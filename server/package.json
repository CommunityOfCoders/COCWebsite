{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for front end",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app",
    "pretest": "eslint --ignore-path .gitignore . ",
    "test": "NODE_ENV=test mocha --recursive --timeout 120000 --exit",
    "serve": "nodemon src/app",
    "new_client": "npm start --prefix=../new_client",
    "dev": "concurrently \" npm run serve \" \" npm run new_client \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix=../new_client && npm run build --prefix=../new_client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cloudinary": "^1.15.0",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "ejs": "^3.1.5",
    "express": "^4.16.4",
    "express-validator": "^6.10.0",
    "helmet": "^4.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-schedule": "^1.3.2",
    "node-waf": "^1.0.0",
    "nodemailer": "^6.4.11",
    "npm": "^6.14.7",
    "password-hasher": "^1.0.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-strongloop": "^2.1.0",
    "mocha": "^7.1.2",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^2.0.3"
  }
}
